CREATE TABLE full_data (
  imei text,
  date_hour timestamp,
  message_type text,
  version text,
  fix int,
  lat double,
  lon double,
  speed double,
  device_time timestamp,
  server_time timestamp,
  io_one float,
  io_two float,
  io_three float,
  io_four float,
  io_five float,
  io_six float,
  io_seven float,
  io_eight float,
  signal_strength int,
  supply_voltage float,
  PRIMARY KEY ((imei, date_hour), device_time)
);


CREATE TABLE last_data (
  imei text,
  message_type text,
  version text,
  fix int,
  lat double,
  lon double,
  speed double,
  device_time timestamp,
  server_time timestamp,
  io_one float,
  io_two float,
  io_three float,
  io_four float,
  io_five float,
  io_six float,
  io_seven float,
  io_eight float,
  signal_strength int,
  supply_voltage float,
  day_max_speed float,
  day_max_speed_time timestamp,
  last_halt_time timestamp,
  PRIMARY KEY (imei)
);
